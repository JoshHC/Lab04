@model IEnumerable<Lab_04_JosueHigueros_PabloAlvarado.Models.Classes.Estampa>

@{
    ViewBag.Title = "Diccionario 2";
}

<h2>Album Panini 2018</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Numero)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Obtenida)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Numero)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Obtenida)
        </td>
        <td>
            @Html.ActionLink("Editar", "Modificar", new {Nombre = item.Nombre, Numero = item.Numero, Obtenida = item.Obtenida})
        </td>
    </tr>
}

</table>
