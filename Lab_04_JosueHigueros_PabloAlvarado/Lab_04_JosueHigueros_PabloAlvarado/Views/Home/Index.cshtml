@{
    ViewBag.Title = "Pagina de Inicio";
}

<div class="jumbotron">
    <header><h1>Album Rusia 2018</h1></header>
    <p class="lead">Lleva el control de las estampitas del Mundial con Nuestra Apicación</p>
    <p><a href="http://es.fifa.com/" class="btn btn-primary btn-lg">Mas Sobre FIFA &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-6">
        <h2>Diccionario 1</h2>
        <p>
            Ingresa las Estampitas, verlos en el Album de la FIFA WOLRD CUP 2018.
        </p>
        <p><a class="btn btn-primary btn-lg" @Html.ActionLink("Ingresar Estampitas", "UploadFile", "Pagina")</a></p>
    </div>

<div class="col-md-6">
    <h2>Diccionario 2</h2>
    <p>
        Ingresa las Estampitas, verlos en el Album de la FIFA WOLRD CUP 2018.
    </p>
    <p><a class="btn btn-primary btn-lg" @Html.ActionLink("Ingresar Estampitas", "UploadFileEstampa", "Pagina")</a></p>
</div>
</div>
